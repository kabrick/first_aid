<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kabricks.firstaidapp.EmergencyFragmentFiles.AbcActFast"
    android:background="@color/mediumGreen">

    <VideoView
        android:layout_width="wrap_content"
        android:layout_height="197dp"
        android:id="@+id/videoView7"
        android:layout_gravity="center_horizontal|top"
        android:layout_above="@+id/textView23"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="125dp" />

    <TextView
        android:layout_width="328dp"
        android:layout_height="102dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Establish what they have taken. When? And how much? Call 911"
        android:id="@+id/textView23"
        android:layout_gravity="center"
        android:layout_above="@+id/textView64"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="119dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="153dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Do not make the person sick"
        android:id="@+id/textView64"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
